
#Path to ohmyzsh
export ZSH=$HOME/.oh-my-zsh
#Theme
ZSH_THEME=ys
#Case Sensative
CASE_SENSITIVE="true"
#Waiting Dots
COMPLETION_WAITING_DOTS="true"
